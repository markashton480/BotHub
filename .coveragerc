[run]
source = .
omit =
    */migrations/*
    */tests/*
    */venv/*
    */.venv/*
    */virtualenv/*
    manage.py
    */apps.py
    */admin.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
